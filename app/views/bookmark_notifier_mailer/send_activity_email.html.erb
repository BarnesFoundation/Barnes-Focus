
<table border="0" cellpadding="0" cellspacing="0" width="100%" style="border: 1px solid #eeeeee;" height="100%">
  <tr>
    <td>
      <div align="center">
        <table border="0" cellpadding="0" cellspacing="0" width="80%" height="100%" class="main-container">
          <tr>
            <td>
              <!-- top spacer -->
              <table cellpadding="0" cellspacing="0" border="0" style="width:100%">
                <tr>
                  <td>
                    <p style="padding: 20px 0">&nbsp;</p>
                  </td>
                </tr>
                <tr><td style="border-top: 1px solid #eeeeee;"></td></tr>
              </table>

              <!-- logo and social icons  -->
              <table cellpadding="0" cellspacing="0" border="0" style="width:100%">
                <tr>
                  <td align="left" style="text-align: left; padding: 20px 0;">
                    <a href="https://collection.barnesfoundation.org" target="_blank">
                      <img alt="Barnes" src="https://barnes-assets.s3.amazonaws.com/barneslogo.svg" width="160px" height="64px" />
                    </a>
                  </td>
                  <td align="right" style="text-align: right; padding: 20px 0;">
                    <a href="https://twitter.com/the_barnes" target="_blank">
                      <img alt="twitter" src="https://www.gstatic.com/youtube/img/email/twitter_email_icon_grey.png" width="20px" height="20px" style="padding: 20px 10px; opacity: 0.8;" />
                    </a>
                    <a href="https://www.youtube.com/user/BarnesFoundation" target="_blank">
                      <img alt="youtube" src="https://www.gstatic.com/youtube/img/email/youtube_email_icon_grey.png" width="20px" height="15px" style="padding: 20px 10px; opacity: 0.8;" />
                    </a>
                  </td>
                </tr>
                <tr><td colspan="2" style="border-top: 1px solid #eeeeee;"></td></tr>
              </table>

              <!-- Welcome and bookmark message -->
              <table cellpadding="0" cellspacing="0" border="0" style="width:100%">
                <tr>
                  <td align="left" style="text-align: left; padding: 20px 0;">
                    <p style="font-size: 40px;  margin: 10px 0; color: #282828;">Bookmarked art</p>
                  </td>
                </tr>
                <tr>
                  <td align="left" style="text-align: left; padding: 20px 0;">
                    <p style="font-size: 22px; color:#B6B6B6; margin: 10px 0;">
                      Thank you for visiting the Barnes today! Here are all the works you bookmarked during your visit:
                    </p>
                  </td>
                </tr>
                <tr><td style="border-top: 1px solid #eeeeee;"></td></tr>
              </table>

              <!-- Bookmarks -->
              <table cellpadding="0" cellspacing="0" border="0" style="width:100%; color: #282828;" class="bookmarks-table">
                <% @els_arr.each do | els_elem | %>
                
                  <tr style="padding: 40px 0;">
                    <td width="25%">
                      <%= image_tag "https://barnes-image-repository.s3.amazonaws.com/images/" + els_elem['id'].to_s + "_" + els_elem['imageSecret'] + "_n.jpg", alt: els_elem['id'].to_s, style:"height: 175px; width: 75%; padding: 40px 0;"%>
                    </td>
                    <td width="75%" style="color: #282828; vertical-align: top; padding-top: 40px;">
                      <p>
                      <%= link_to 'https://collection.barnesfoundation.org/objects/'+ els_elem['id'].to_s, :title => "barnes collection" do %>
                        <h3><%= els_elem['title'] %></h3>
                      <% end %>
                      </p>
                    
                      <%= els_elem['people']%>. <%= els_elem['title']%>, <%=els_elem['displayDate']%>. <%=els_elem['medium']%>, <%=els_elem['dimensions'] %>. <%=els_elem['invno'] %>
                     
                      <% if els_elem['objRightsTypeID'].present? %>
                        <% if ['1', '3'].include?(els_elem['objRightsTypeID']) %>
                          <p>In Copyright. <br />
                            <%= els_elem['creditLine'] %>
                          </p>
                        <% elsif ['4', '8', '10'].include?(els_elem['objRightsTypeID']) %>
                          <p>Public domain. </p>
                        <% elsif ['2', '6'].include?(els_elem['objRightsTypeID']) %>
                          <p>Copyright Undetermined</p>
                        <% end %>
                      <% end %>
                    </td>
                  </tr>
                  <tr><td colspan="2" style="border-top: 1px solid #eeeeee;"></td></tr>
                <% end %>
              </table>

              <!-- footer spacer-->
              <table cellpadding="0" cellspacing="0" border="0" style="width:100%">
                <tr>
                  <td>
                    <p>&nbsp;</p>
                  </td>
                </tr>
              </table>

              <!-- footer social icons -->
              <table cellpadding="0" cellspacing="0" border="0" style="width:100%" class="footer-social">
                <tr>
                  <td>
                    <p style="padding: 20px 0">&nbsp;</p>
                  </td>
                  <td align="center">
                    <a href="https://twitter.com/the_barnes" target="_blank">
                      <img alt="twitter" src="https://www.gstatic.com/youtube/img/email/twitter_email_icon_grey.png" width="20px" height="20px" style="padding: 20px 10px; opacity: 0.8;"/>
                    </a>
                    <a href="https://www.youtube.com/user/BarnesFoundation" target="_blank">
                      <img alt="youtube" src="https://www.gstatic.com/youtube/img/email/youtube_email_icon_grey.png" width="20px" height="15px" style="padding: 20px 10px; opacity: 0.8;"/>
                    </a>
                  </td>
                  <td>
                    <p style="padding: 20px 0">&nbsp;</p>
                  </td>
                </tr>
              </table>

              <!-- footer links -->
              <div align="center">
                <table cellpadding="0" cellspacing="0" border="0" style="width: 60%; table-layout: fixed;" class="footer-action">
                  <tr style="padding: 0 40px;">
                    <td style="width:20%; text-align: center; vertical-align: middle;">
                      <h3><a href="https://www.barnesfoundation.org/whats-on/collection" target="_blank">Tickets</a></h3>
                    </td>
                    <td style="width:20%; text-align: center; vertical-align: middle;">
                      <h3><a href="https://www.barnesfoundation.org/whats-on/talks" target="_blank">Tours</a></h3>
                    </td>
                    <td style="width:20%; text-align: center; vertical-align: middle;">
                      <h3><a href="https://www.barnesfoundation.org/support" target="_blank">Join</a></h3>
                    </td>
                    <td style="width:20%; text-align: center; vertical-align: middle;">
                      <h3><a href="https://shop.barnesfoundation.org/" target="_blank">Shop</a></h3>
                    </td>
                    <td style="width:20%; text-align: center; vertical-align: middle;">
                      <h3><a href="https://www.barnesfoundation.org/support" target="_blank">Membership</a></h3>
                    </td>
                  </tr>
                </table>
              </div>

              <!-- footer address | contact etc -->
              <div align="center">
                <table cellpadding="0" cellspacing="0" border="0" style="width: 50%; table-layout: fixed;" class="footer-info">
                  <tr>
                    <td style="text-align: center; vertical-align: middle;">
                      <div style="height:28px; color: #b8b8b8;">2025 Benjamin Franklin Parkway, Philadelphia, PA 19130</div>
                    </td>
                  </tr>
                  <tr>
                    <td style="text-align: center; vertical-align: middle;">
                      <div style="height:28px; color: #b8b8b8;"><a href="tel:215-278-7000">215-278-7000</a> &nbsp;&nbsp;&nbsp;mailto: info@barnesfoundation.org</div>
                    </td>
                  </tr>
                  <tr>
                    <td style="text-align: center; vertical-align: middle;">
                      <div style="height:28px; color: #b8b8b8;">
                        <a href="https://www.barnesfoundation.org/privacy-policy" style="text-decoration: underline;">Privacy Policy&nbsp;&nbsp;&nbsp;|</a>
                        <a href="http://www.pages03.net/thebarnesfoundation/EmailPreferences/curate_your_inbox" style="text-decoration: underline;">&nbsp;&nbsp;&nbsp;Email Preferences&nbsp;&nbsp;&nbsp;|</a>
                        <a href="http://www.pages03.net/thebarnesfoundation/EmailPreferences/opt-out" style="text-decoration: underline;">&nbsp;&nbsp;&nbsp;Unsubscribe</a>
                      </div>
                    </td>
                  </tr>
                </table>
              </div>

               <!-- footer spacer-->
              <table cellpadding="0" cellspacing="0" border="0" style="width:100%">
                <tr>
                  <td>
                    <p>&nbsp;</p>
                  </td>
                </tr>
                <tr><td style="border-top: 1px solid #eeeeee;"></td></tr>
              </table>

               <!-- footer spacer-->
              <table cellpadding="0" cellspacing="0" border="0" style="width:100%">
                <tr>
                  <td>
                    <p>&nbsp;</p>
                  </td>
                </tr>
              </table>


            </td>
          </tr>
        </table>
      </div>
    </td>
  </tr>  
</table>