commands:
    00_start_yarn_script:
        command: echo "Starting yarn steps..."; mkdir -p /home/my_random_dir_1
    01_download_node:
        command: curl --silent --location https://rpm.nodesource.com/setup_12.x | sudo bash -
    02_install_node: 
        command: yum -y install nodejs
    03_download_yarn:
        command: wget https://dl.yarnpkg.com/rpm/yarn.repo -O /etc/yum.repos.d/yarn.repo;
    04_install_yarn:
        command: yum -y install yarn;
    05_install_packages:
        command: echo $PWD; yarn; echo $(ls)
        cwd: $(/opt/elasticbeanstalk/bin/get-config platformconfig -k AppStagingDir)
    06_make_webapp_dir:
        command: chown webapp:webapp .bundle/config; mkdir -p /home/webapp
    07_provide_permissions:
        command: chown webapp:webapp /home/webapp; chmod 700 /home/webapp
